{
  "name": "t_connect",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --tasks installDevelopmentDebug --appIdSuffix uat --active-arch-only",
    "android:dev_clean": "run android:dev --tasks 'uninstallDevelopmentDebug,installDevelopmentDebug'",
    "android": "run android:dev",
    "android_clean": "run android:dev_clean",
    "and": "run android:dev",
    "andc": "run android:dev_clean",
    "android:dev-release": "react-native run-android --tasks installDevelopmentRelease --appIdSuffix uat --active-arch-only",
    "android:dev-release_clean": "run android:dev-release --tasks 'uninstallDevelopmentRelease,installDevelopmentRelease'",
    "andr": "run android:dev-release",
    "andrc": "run android:dev-release_clean",
    "android:dev-build": "react-native build-android --tasks clean,assembleDevelopmentRelease",
    "andb": "run android:dev-build",
    "android:prod": "react-native run-android --tasks installProductionDebug --active-arch-only",
    "android:prod_clean": "run android:prod --tasks 'uninstallProductionDebug,installProductionDebug'",
    "andp": "run android:prod",
    "andpc": "run android:prod_clean",
    "android:prod-release": "react-native run-android --tasks installProductionRelease --active-arch-only",
    "android:prod-release_clean": "run android:prod-release --tasks 'uninstallProductionRelease,installProductionRelease'",
    "andpr": "run android:prod-release",
    "andprc": "run android:prod-release_clean",
    "android:prod-build": "react-native build-android --tasks clean,assembleProductionRelease",
    "andpb": "run android:prod-build",
    "android:prod-bundle": "react-native build-android --tasks clean,bundleProductionRelease",
    "andbd": "run android:prod-bundle",
    "ios:dev": "pod-install && react-native run-ios --mode Debug --scheme Development",
    "ios": "run ios:dev",
    "ios:dev-release": "pod-install && react-native run-ios --mode Release --scheme Development",
    "iosr": "run ios:dev-release",
    "ios:dev-build": "pod-install && ./scripts/build-ios --mode Release --scheme Development",
    "iosb": "run ios:dev-build",
    "ios:prod": "pod-install && react-native run-ios --mode Debug --scheme Production",
    "iosp": "run ios:prod",
    "ios:prod-release": "pod-install && react-native run-ios --mode Release --scheme Production",
    "iospr": "run ios:prod-release",
    "ios:prod-build": "pod-install && ./scripts/build-ios --mode Release --scheme Production",
    "iospb": "run ios:prod-build",
    "start": "concurrently -Pkr \"yarn react-native start --experimental-debugger {@}\" \"yarn import-assets --watch\" --",
    "st": "run start",
    "str": "run start --reset-cache",
    "connect": "adb reverse tcp:${RCT_METRO_PORT:-8081} tcp:${RCT_METRO_PORT:-8081}",
    "cn": "run connect",
    "constart": "run connect && run start",
    "cst": "run constart",
    "test": "jest",
    "gen": "hygen gen-template",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "lint-check": "eslint {{src,tests}/**/*.{ts,tsx},index.js,src/i18n/resources/*/*.json} --cache --no-error-on-unmatched-pattern && ls-lint",
    "lint-fix": "eslint {{src,tests}/**/*.{ts,tsx},index.js,src/i18n/resources/*/*.json} --fix --config eslint.config-autofix.js --cache --cache-location .eslintcache-autofix --no-error-on-unmatched-pattern",
    "format-check": "editorconfig-checker; prettier --check . --cache --cache-location .prettiercache --no-error-on-unmatched-pattern",
    "type-check": "tsc",
    "clean": "yarn react-native clean",
    "validateinstall": "node ./scripts/enforceEngines.js",
    "postinstall": "husky && git config --local include.path '../.gitconfig' && node ./scripts/updatePatchVersions.js && patch-package",
    "bootsplash": "react-native generate-bootsplash src/assets/images/bootsplash/bootsplash.png --platforms=android,ios --background=#0254db --assets-output=src/assets/images/bootsplash",
    "upver": "node ./scripts/version.js",
    "import-assets": "node ./scripts/importer"
  },
